<table>
  <thead >
    <tr>
      <th>Title</th>
      <th>Level</th>
      <th>Questions count</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
  <% @tests.each do |test| %>
    <tr>
      <td><%= link_to test.title,  test_path(test)%></td>
      <td><%= test.level %></td>
      <td><%= test.questions.count %></td>
      <td><%= button_to "Start", start_test_path(test) %></td>
    </tr>
  <% end %>
  </tbody>
</table>
